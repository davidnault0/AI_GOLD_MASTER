# AI Model Configuration
# Copy this file to .env and fill in your values

# OpenAI GPT-4o Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=0.7

# Qwen 3 Local Configuration
QWEN3_ENDPOINT=http://localhost:8000
QWEN3_MODEL=qwen-3
QWEN3_MAX_TOKENS=2048
QWEN3_TEMPERATURE=0.7

# Default AI Model Selection
# Options: gpt-4o, qwen-3
DEFAULT_AI_MODEL=gpt-4o

# Hybrid Mode Configuration
# Enable to automatically route tasks to optimal model
HYBRID_MODE_ENABLED=false

# Network Configuration
API_TIMEOUT=30000
MAX_RETRIES=3
RATE_LIMIT_PER_MINUTE=60

# TradingView Credentials (for compile_pine_script.js)
TRADINGVIEW_EMAIL=your-email@example.com
TRADINGVIEW_PASSWORD=your-secure-password
